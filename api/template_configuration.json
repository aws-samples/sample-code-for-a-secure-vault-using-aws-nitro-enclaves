{
  "Tags": {
    "AppManagerCFNStackKey": "nitro-vault"
  },
  "StackPolicy": {
    "Statement": [
      {
        "Effect": "Deny",
        "Action": "Update:Replace",
        "Principal": "*",
        "Resource": "*",
        "Condition": {
          "StringEquals": {
            "ResourceType": ["AWS::DynamoDB::GlobalTable"]
          }
        }
      },
      {
        "Effect": "Allow",
        "Action": "Update:*",
        "Principal": "*",
        "Resource": "*"
      }
    ]
  }
}
